{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test npm run migrate:undo && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed",
    "test": "cross-env NODE_ENV=test nyc mocha --exit --require @babel/register --timeout 600000 ./server/test/**/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "node build/index.js",
    "build": "npm run remove:build && babel index.js -d build && babel server -d build/server",
    "remove:build": "rm -rf ./build",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "mail": "babel-node ./server/utils/emailer.helper.js",
    "heroku:migrate": "npm run migrate:undo && npm run migrate && npm run seed",
    "heroku-postbuild": "npm run build && npm run heroku:migrate"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "editorconfig": "^0.15.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "nodemailer": "^6.0.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "reading-time": "^1.2.0",
    "sequelize": "^5.1.1",
    "sequelize-cli": "^5.4.0",
    "slug": "^0.9.1",
    "slugify": "^1.3.4",
    "string-strip-html": "^4.0.12",
    "swagger-ui-express": "^4.0.2",
    "underscore": "^1.12.1",
    "validator": "^10.11.0",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.1.2",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
